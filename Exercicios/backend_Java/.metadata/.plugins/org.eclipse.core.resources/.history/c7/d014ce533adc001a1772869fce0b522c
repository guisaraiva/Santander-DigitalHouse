package array_primos;

class Programa {

	public static void main(String[] args) {
				
		int[] numbers = {3,25,35,22,12,35,5,85,11};
			
		int cont;
		
		for (cont = 0; cont < numbers.length; cont ++ ) {
			if (numbers[cont] % 2 == 0) {
				System.out.println("O número: " + numbers[cont] + " não é Primo!");
			}
			if (numbers[cont] % 5 >= 2) {
				System.out.println("O número: " + numbers[cont] + " não é Primo!");
			}
				
		}
		
	}

}
